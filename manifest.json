{
  "manifest_version": 3,
  "name": "SaveMyExams AutoDark",
  "version": "1.0",
  "description": "Automatically switch SaveMyExams theme based on system appearance.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.savemyexams.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
